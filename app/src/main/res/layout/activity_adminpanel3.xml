<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".adminpanel3"
    android:background="@color/grey"
    >

    <LinearLayout
        android:id="@+id/mainln"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_margin="20dp">

        <TextView
            android:id="@+id/textView28"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textSize="32sp"
            android:textColor="@color/white"
            android:layout_marginTop="30dp"
            android:gravity="center"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="50dp"
            android:padding="2dp"

            android:background="@drawable/roundblubord"
            android:layout_marginBottom="50dp">

            <LinearLayout
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:background="@drawable/roundgrayleft"
                android:paddingTop="30dp"
                android:elevation="10dp"
                >

                <FrameLayout
                    android:id="@+id/f1"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginVertical="10dp"
                    android:background="@color/grey7"
                    android:onClick="y1">

                    <TextView
                        android:id="@+id/textView85"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="TextView"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:textColor="@color/white"
                        android:fontFamily="@font/thin"/>
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/fy2"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"

                    android:layout_marginBottom="10dp"
                    android:background="@color/grey7"
                    android:onClick="y2">

                    <TextView
                        android:id="@+id/textView86"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="TextView"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:textColor="@color/white"
                        android:fontFamily="@font/thin"
                        />
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/fy3"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@color/grey7"
                    android:layout_marginBottom="10dp"
                    android:onClick="y3"
                    >

                    <TextView
                        android:id="@+id/textView87"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="TextView"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:textColor="@color/white"
                        android:fontFamily="@font/thin" />
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/fy4"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@color/grey7"
                    android:onClick="y4"
                    >

                    <TextView
                        android:id="@+id/textView88"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="TextView"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:textColor="@color/white"
                        android:fontFamily="@font/thin"
                        />
                </FrameLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingHorizontal="20dp"
                android:paddingTop="30dp"
                android:elevation="10dp">


                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="@drawable/roundgraybord"
                    android:elevation="10dp"
                    android:onClick="AddGroup"
                    >

                    <TextView
                        android:id="@+id/textView89"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Add group"

                        android:gravity="center"
                        android:layout_gravity="center"
                        android:textColor="@color/white"
                        android:fontFamily="@font/thin"
                        android:textSize="18sp"/>
                </FrameLayout>

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingTop="20dp">

                    <LinearLayout
                        android:id="@+id/group_group"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/textView127"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Сдесь нету групп"
                            android:gravity="center"/>
                    </LinearLayout>
                </ScrollView>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <FrameLayout
        android:id="@+id/panini"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>